{
    "name": "Annota",
    "version": "1.0",
    "description": "Annotation Orginization System.",
    "permissions": ["storage", "contextMenus", "tabs", "*://*/*"],
    "manifest_version": 2,
    "browser_action": {
      "default_icon": {
        "128": "annota-logo.png"
      }
    },
    "chrome_url_overrides": {
      "newtab": "app/templates/home.html"
    },
    "icons": {
        "128": "annota-logo.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [

    ],
    "web_accessible_resources": [
      "overlay/overlay.html"
    ]
}
